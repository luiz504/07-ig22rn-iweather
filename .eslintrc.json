{
  "extends": "@rocketseat/eslint-config/react",
  "rules": {
    "no-console": "warn",
    "camelcase": "off"
  },
  "ignorePatterns": ["*.js"],
  "overrides": [
    {
      // Test files only
      "files": [
        "**/__tests__/**/*.[jt]s?(x)",
        "**/?(*.)+(spec|test).[jt]s?(x)"
      ],
      "extends": ["plugin:testing-library/react"]
    }
  ]
}
